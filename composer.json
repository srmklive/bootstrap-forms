{
    "name": "srmklive/bootforms",
    "description": "Just a Formbuilder with some Bootstrap specific conveniences. Remembers old input, retrieves error messages and handles all your boilerplate Bootstrap markup automatically.",
    "authors": [
        {
            "name": "Raza Mehdi",
            "email": "srmk@outlook.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "~5.1|~5.2|~5.3|~5.4|~5.5|~5.6|~5.7|~5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7|~6.0|~7.0",
        "mockery/mockery": "~0.9|~1.0"
    },
    "autoload": {
        "psr-4": {
            "Srmklive\\BootForms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Srmklive\\BootForms\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Srmklive\\BootForms\\BootFormsServiceProvider"
            ],
            "aliases": {
                "BootForm": "Srmklive\\BootForms\\Facades\\BootForm"
            }    
        }        
    },
    "minimum-stability": "stable"
}
